<h2>Menu Makanan</h2>
<br>
<table class="table">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Nama</th>
      <th scope="col">Category</th>
    </tr>
  </thead>
  <tbody>
  <% @menus.each do |menu| %>
    <tr>
      <th scope="row">1</th>
      <td><%= menu.name %></td>
    </tr>
    <% end %>
  </tbody>
</table>