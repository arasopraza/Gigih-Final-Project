<div class="container">
<br>
<h2>Menu Makanan</h2>
<br>
<%= link_to "New ", new_menu_path %>
<table class="table">
  <thead>
    <tr>
      <th scope="col">No</th>
      <th scope="col">Nama</th>
      <th scope="col">Price</th>
    </tr>
  </thead>
  <tbody>
  <% no = 0 %>
  <% @menus.each do |menu| %>
    <tr>
      <th scope="row"><%= no += 1 %></th>
      <td><%= link_to menu.name, menu%></td>
      <td><%= menu.price %></td>
    </tr>
    <% end %>
  </tbody>
</table>
</div>